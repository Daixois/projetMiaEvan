{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<div class="container my-4">
	
{% for theme in themes %}
	{% if theme.persos|length == 6 %}
			<a href="{{ path('memory', {id : theme.id}) }}" class="card">
			<div class="card-body" id="home">
				<img src="{{ vich_uploader_asset(theme, 'imageFile') }}" class="card-img-top" alt="{{theme.name}}">
				<h5 class="card-nom">{{theme.name}}</h5>
			</div>
		{% endif %}
{% endfor %}

 

{% endblock %}
